<h1> ALL EVENTS </h1>

<p> <%= link_to "Add event", new_event_path %> </p>

<% @events.each do |event| %>
    <div>
        <h4><%= link_to event.title, event %></h4>
        <p><%= event.start_date %></p>
        <p><%= event.end_date %></p>
        <p><%= event.venue %>, <%= event.location %></p>
    <div>
<% end %>